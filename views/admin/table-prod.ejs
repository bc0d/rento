<!doctype html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang=""> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8" lang=""> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9" lang=""> <![endif]-->
<!--[if gt IE 8]><!-->
<html class="no-js" lang="en">
<!--<![endif]-->

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>RENTO</title>
    <meta name="description" content="Sufee Admin - HTML5 Admin Template">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <% include partials/exturl %>



</head>

<body>
    <!-- Left Panel -->
    <% include partials/sidepanel %>

    <!-- /#left-panel -->

    <!-- Left Panel -->

    <!-- Right Panel -->

    <div id="right-panel" class="right-panel">

        <!-- Header-->
        <% include partials/header %>
        
                <!-- Header-->

        <div class="breadcrumbs">
            <div class="col-sm-4">
                <div class="page-header float-left">
                    <div class="page-title">
                        <h1>Dashboard</h1>
                    </div>
                </div>
            </div>
            <div class="col-sm-8">
                <div class="page-header float-right">
                    <div class="page-title">
                        <ol class="breadcrumb text-right">
                            <li><a href="#">Dashboard</a></li>
                            <li><a href="#">Table</a></li>
                            <li class="active">Basic table</li>
                        </ol>
                    </div>
                </div>
            </div>
        </div>

        <div class="content mt-3">
            <div class="animated fadeIn">
                <div class="row">


                    <div class="col-lg-12">
                        <div class="card">
                            <div class="card-header">
                                <strong class="card-title">Product</strong>
                            </div>
                            <div class="card-body">
                                <table class="table table-dark">
                                    <thead>
                                        <tr>
                                            <th scope="col">Product ID</th>
                                            <th scope="col">Product Name</th>
                                            <th scope="col">Catogary</th>
                                            <th scope="col">Renter ID</th> 
                                            <th scope="col">MRP</th>
                                            <th scope="col">RA</th>
                                            <th scope="col">RP</th>
                                            <th scope="col">Desc</th>
                                            <th scope="col">Instr</th>
                                            <th scope="col">IMG 1</th>
                                            <th scope="col">IMG 2</th>
                                            <th scope="col">IMG 3</th>
                                            <th scope="col">IMG 4</th>
                                            <th scope="col">Location</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% 
                                            prod.forEach(function(prod) {
                                        %>
                                        <tr>
                                            <th><%=prod.prod_id %></th>
                                            <td><%=prod.pdt_name %></td>
                                            <td><%=prod.category %></td>
                                            <td><%=prod.renter_id %></td>
                                            <td><%=prod.max_rent_prd %></td>
                                            <td><%=prod.rent_amt %></td>
                                            <td><%=prod.rent_per %></td>
                                            <td><%=prod.descr %></td>
                                            <td><%=prod.instr %></td>
                                            <td><%=prod.pic1 %></td>   
                                            <td><%=prod.pic2 %></td>
                                            <td><%=prod.pic3 %></td>
                                            <td><%=prod.pic4 %></td>
                                            <td><%=prod.pickup_point %></td>
                                        </tr>
                                        <%
                                            });
                                        %>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div><!-- .animated -->
        </div><!-- .content -->


    </div><!-- /#right-panel -->

    <!-- Right Panel -->


        
          <!-- script -->
    <% include partials/extscript %>


</body>

</html>
