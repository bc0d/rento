<html class="no-js" lang="en">
<!--<![endif]-->

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>RENTO</title>
    <meta name="description" content="Sufee Admin - HTML5 Admin Template">
    <meta name="viewport" content="width=device-width, initial-scale=1">

   <% include partials/exturl %>



</head>

<body>
    <!-- Left Panel -->
    <% include partials/sidepanel %>

    <!-- /#left-panel -->

    <!-- Left Panel -->

    <!-- Right Panel -->

    <div id="right-panel" class="right-panel">

        <!-- Header-->
        <% include partials/header %>
        
                <!-- Header-->

        <div class="breadcrumbs">
            <div class="col-sm-4">
                <div class="page-header float-left">
                    <div class="page-title">
                        <h1>Dashboard</h1>
                    </div>
                </div>
            </div>
            <div class="col-sm-8">
                <div class="page-header float-right">
                    <div class="page-title">
                        <ol class="breadcrumb text-right">
                            <li><a href="#">Dashboard</a></li>
                            <li><a href="#">Table</a></li>
                            <li class="active">Basic table</li>
                        </ol>
                    </div>
                </div>
            </div>
        </div>

        <div class="content mt-3">
            <div class="animated fadeIn">
                <div class="row">


                    <div class="col-lg-12">
                        <div class="card">
                            <div class="card-header">
                                <strong class="card-title">User</strong>
                            </div>
                            <div class="card-body">
                                <table class="table table-dark">
                                    <thead>
                                        <tr>
                                            <th scope="col">User ID</th>
                                            <th scope="col">First Name</th>
                                            <th scope="col">Second Name</th>
                                            <th scope="col">Mail ID</th>
                                            <th scope="col">Phone</th>
                                            <th scope="col">Photo</th>
                                            <th scope="col">ID IMG1</th>
                                            <th scope="col">ID IMG2</th>
                                            <th scope="col">Address</th>
                                            <th scope="col">User_Cat</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% 
                                            usr.forEach(function(usr) {
                                        %>
                                        <tr>
                                            <th><%=usr.user_id %></th>
                                            <td><%=usr.f_name %></td>
                                            <td><%=usr.s_name %></td>
                                            <td><%=usr.mail %></td>
                                            <td><%=usr.ph_no %></td>
                                            <td><%=usr.user_pic %></td>
                                            <td><%=usr.id_pic1 %></td>
                                            <td><%=usr.id_pic2 %></td>
                                            <td><%=usr.address %></td>
                                            <td><%=usr.privilege %></td>
                                        </tr>
                                        <%
                                            });
                                        %>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div><!-- .animated -->
        </div><!-- .content -->


    </div><!-- /#right-panel -->

    <!-- Right Panel -->


         
          <!-- script -->
    <% include partials/extscript %>


</body>

</html>
